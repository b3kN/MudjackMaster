    "@chakra-ui/color-mode": "^2.2.0",
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.15.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@next/env": "^15.2.4",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "critters": "^0.0.25",
