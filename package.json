{
  "name": "mudjacker",
  "version": "0.0.1",
  "private": true,
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest --coverage --silent --no-file-parallelism",
